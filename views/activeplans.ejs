    <div class="container-fluid animatedParent" style="
          background-color: rgba(128, 216, 89, 0.8);
          border-radius: 10px;
          width: 90%;
          padding: 10px;
          margin: 20px auto;
        ">
      <h3 style="font-size: 1.8em">Deposits</h3>
      <% if(deposits.length > 0) { %>
      <table id="table">
        <thead>
          <tr>
            <td><b class="name">Amount</b></td>
            <td><b class="name">Payment mode</b></td>
            <td><b class="name">Status</b></td>
            <td><b class="name">Date made</b></td>
          </tr>
        </thead>
        <tbody>
          <% deposits.forEach((deposit)=>{ %>
          <tr>
            <td><b><%=`$${deposit.amount}`%></b></td>
            <td><%=`${deposit.paymentMode}`%></td>
            <td><%=`${deposit.status}`%></td>
            <td><%= deposit.createdAt.toString().slice(0,15) %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
      <% } else { %>
      <div style="background-color: cornsilk">
        <h4>No deposits made yet</h4>
        <a href="deposit">Fund account now</a>
      </div>
      <% } %>
    </div>
    <div class="container-fluid animatedParent" style="
          background-color: rgba(128, 216, 89, 0.8);
          border-radius: 10px;
          width: 90%;
          padding: 10px;
          margin: 20px auto;
        ">
      <h3 style="font-size: 1.8em">My investments</h3>
      <% if(investments.length > 0) { %>
      <table id="table">
        <thead>
          <tr>
            <td><b class="name">Plan</b></td>
            <td><b class="name">Amount</b></td>
            <td><b class="name">Status</b></td>
            <td><b class="name">Duration</b></td>
            <td><b class="name">Activation date</b></td>
          </tr>
        </thead>
        <tbody>
          <% investments.forEach((investment)=>{ %>
          <tr>
            <td><b><%=`${investment.tier}`%></b></td>
            <td><%=`$${investment.amountInvested}`%></td>
            <td><%=`${investment.status}`%></td>
            <td><%=`${investment.duration}`%></td>
            <td><%= investment.createdAt.toString().slice(0,15) %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
      <% } else { %>
      <div style="background-color: cornsilk">
        <h4>No active investments yet</h4>
        <a href="deposit">Fund account now</a><span> to purchase a plan and start earning</span>
      </div>
      <% } %>
    </div>
    <div class="container-fluid animatedParent" style="
          background-color: rgba(128, 216, 89, 0.8);
          border-radius: 10px;
          width: 90%;
          padding: 10px;
          margin: 20px auto;
        ">
      <h3 style="font-size: 1.8em">Withdrawal requests</h3>
      <% if(withdrawals.length > 0) { %>
      <table id="table">
        <thead>
          <tr>
            <td><b class="name">Amount</b></td>
            <td><b class="name">Status</b></td>
            <td><b class="name">Payment Mode</b></td>
            <td><b class="name">Date</b></td>
          </tr>
        </thead>
        <tbody>
          <% withdrawals.forEach((withdrawal)=>{ %>
          <tr>
            <td><%=`$${withdrawal.amountRequested}`%></td>
            <td><%=`${withdrawal.status}`%></td>
            <td><%=`${withdrawal.paymentMode}`%></td>
            <td><%= withdrawal.createdAt.toString().slice(0,15) %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
      <% } else { %>
      <div style="background-color: cornsilk">
        <h4>You have no withdrawal requests yet</h4>
      </div>
      <% } %>
    </div>
